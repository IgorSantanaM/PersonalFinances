{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoSettings": {
    "ConnectionString": "mongodb://mongodb:27017",
    "DatabaseName": "PersonalFinances"
  },
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "ValidAudiences": [
          "personalfinances-webapi"
        ],
        "ValidIssuers": "the_issuer_of_token"
      }
    }
  },
  "AllowedHosts": "*"
}
